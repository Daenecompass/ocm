<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.pikasoftware.cms-csv-download</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/php</string>
        <string>%%[script_path]%%/cms-csv-download.php</string>
    </array>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Minute</key>
        <integer>30</integer>
        <key>Hour</key>
        <integer>02</integer>
    </dict>
</dict>
</plist>